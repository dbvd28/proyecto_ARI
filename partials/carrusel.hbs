<div id="carousel_rescatados" class="carousel slide" data-bs-ride="carousel">
    <h2 class="text-center mb-4 text-black">🐕 Perros rescatados
    </h2>
<div class="carousel-inner">
    {{#each photos}}
      <div class="carousel-item {{#if @first}}active{{/if}}">
        <img src="{{imgSrc}}" class="{{imgCls}}" alt="{{imgDsc}}">
      </div>
    {{/each}}
  </div>
</div>